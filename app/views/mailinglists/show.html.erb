<div class="content-wrap">
    <!-- main page content. the place to put widgets in. usually consists of .row > .col-md-* > .widget.  -->
    <main id="content" class="content" role="main">
        <ol class="breadcrumb" style="font-size:16px;">
            <li>Tongsu</li>
            <li class="active">Mailing List</li>
        </ol>
        <h2 class="page-title"><%= @channel.group.name %> - <span class="fw-semi-bold"><%= @channel.name %></span></h2>

        <ul class="timeline" id="my-posts">
            <%= render @mailinglists %>
        </ul>

        <div class="timeline" id="infinite-scrolling" style="text-align: center;">
          <%= will_paginate %>
        </dvv>
    </main>
</div>

